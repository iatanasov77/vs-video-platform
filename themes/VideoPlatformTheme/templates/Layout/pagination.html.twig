{% if pageCount > 1 %}
    <ul class="paginator">
	    {% if previous is defined %}
	        <li class="paginator__item paginator__item--prev">
                <a href="{{ path(route, query|merge({(pageParameterName): previous})) }}">
                    <svg xmlns="http://www.w3.org/2000/svg" enable-background="new 0 0 24 24" viewBox="0 0 24 24"><path d="M8.5,12.8l5.7,5.6c0.4,0.4,1,0.4,1.4,0c0,0,0,0,0,0c0.4-0.4,0.4-1,0-1.4l-4.9-5l4.9-5c0.4-0.4,0.4-1,0-1.4c-0.2-0.2-0.4-0.3-0.7-0.3c-0.3,0-0.5,0.1-0.7,0.3l-5.7,5.6C8.1,11.7,8.1,12.3,8.5,12.8C8.5,12.7,8.5,12.7,8.5,12.8z"/></svg>
                </a>
            </li>
	    {% else %}
	        <li class="disabled">
	            <span>&laquo;&nbsp;{{ 'label_previous'|trans({}, 'KnpPaginatorBundle') }}</span>
	        </li>
	    {% endif %}
	
	
	    {% if startPage > 1 %}
	        <a href="{{ path(route, query|merge({(pageParameterName): 1})) }}">1</a>
	
	        {% if startPage == 3 %}
	            <a href="{{ path(route, query|merge({(pageParameterName): 2})) }}">2</a>
	        {% elseif startPage != 2 %}
	            <span>&hellip;</span>
	        {% endif %}
	    {% endif %}
	
	    {% for page in pagesInRange %}
	        {% if page != current %}
	            <li class="paginator__item"><a href="{{ path(route, query|merge({(pageParameterName): page})) }}">{{ page }}</a></li>
	        {% else %}
                <li class="paginator__item paginator__item--active"><a href="javascript:void(0);">{{ page }}</a></li>
	        {% endif %}
	
	    {% endfor %}
	
	    {% if pageCount > endPage %}
	        {% if pageCount > (endPage + 1) %}
	            {% if pageCount > (endPage + 2) %}
	                <span>&hellip;</span>
	            {% else %}
	                <a href="{{ path(route, query|merge({(pageParameterName): (pageCount - 1)})) }}">{{ pageCount -1 }}</a>
	            {% endif %}
	        {% endif %}
	        <a href="{{ path(route, query|merge({(pageParameterName): pageCount})) }}">{{ pageCount }}</a>
	    {% endif %}
	
	    {% if next is defined %}
	        <li class="paginator__item paginator__item--next">
                <a href="{{ path(route, query|merge({(pageParameterName): next})) }}">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M15.54,11.29,9.88,5.64a1,1,0,0,0-1.42,0,1,1,0,0,0,0,1.41l4.95,5L8.46,17a1,1,0,0,0,0,1.41,1,1,0,0,0,.71.3,1,1,0,0,0,.71-.3l5.66-5.65A1,1,0,0,0,15.54,11.29Z"/></svg>
                </a>
            </li>
	    {% else %}
	        <li class="disabled">
	            <span>{{ 'label_next'|trans({}, 'KnpPaginatorBundle') }}&nbsp;&raquo;</span>
	        </li>
	    {% endif %}
    </ul>
{% endif %}
